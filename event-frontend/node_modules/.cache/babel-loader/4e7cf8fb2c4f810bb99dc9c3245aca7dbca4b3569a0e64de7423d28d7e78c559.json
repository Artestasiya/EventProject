������� �������� �����������\r\n    const handleImageUpload = (e) => {\r\n        const file = e.target.files[0];\r\n        if (file) {\r\n            const reader = new FileReader();\r\n            reader.onload = () => {\r\n                setFormData({ ...formData, image: reader.result });\r\n            };\r\n            reader.readAsDataURL(file);\r\n        }\r\n    };\r\n\r\n    // ���������� �������� �����\r\n    const handleSubmit = async (e) => {\r\n        e.preventDefault();\r\n        setError('');\r\n        setLoading(true);\r\n\r\n        // �������� ���������� ������������ �����\r\n        if (!formData.name || !formData.description || !formData.date || !formData.id_place || !formData.id_category || !formData.max_amount) {\r\n            setError('Please fill in all required fields.');\r\n            setLoading(false);\r\n            return;\r\n        }\r\n\r\n        try {\r\n            const response = await fetch('http://localhost:5000/api/events', {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type': 'application/json',\r\n                },\r\n                body: JSON.stringify(formData),\r\n            });\r\n\r\n            if (!response.ok) {\r\n                const errorData = await response.json();\r\n                setError(errorData.message || 'Failed to add the event.');\r\n                return;\r\n            }\r\n\r\n            // �������� ���������� �������\r\n            navigate('/event'); // ��������������� �� �������� � ���������\r\n        } catch (err) {\r\n            console.error('Error submitting event:', err);\r\n            setError('Server error. Please try again later.');\r\n        } finally {\r\n            setLoading(false);\r\n        }\r\n    };\r\n\r\n    return (\r\n        <div className=\"add-container\" style={{ backgroundImage: `url(${backgroundImage})` }}>\r\n            <form className=\"add-form\" onSubmit={handleSubmit}>\r\n                <h1>Add Event</h1>\r\n                {error && <p className=\"error-message\">{error}</p>}\r\n\r\n                <label htmlFor=\"name\">Event Name</label>\r\n                <input\r\n                    type=\"text\"\r\n                    id=\"name\"\r\n                    name=\"name\"\r\n                    value={formData.name}\r\n                    onChange={handleChange}\r\n                    required\r\n                />\r\n\r\n                <label htmlFor=\"description\">Description</label>\r\n                <textarea\r\n                    id=\"description\"\r\n                    name=\"description\"\r\n                    value={formData.description}\r\n                    onChange={handleChange}\r\n                    required\r\n                ></textarea>\r\n\r\n                <label htmlFor=\"date\">Date</label>\r\n                <input\r\n                    type=\"datetime-local\"\r\n                    id=\"date\"\r\n                    name=\"date\"\r\n                    value={formData.date}\r\n                    onChange={handleChange}\r\n                    required\r\n                />\r\n\r\n                <label htmlFor=\"id_place\">Place ID</label>\r\n                <input\r\n                    type=\"number\"\r\n                    id=\"id_place\"\r\n                    name=\"id_place\"\r\n                    value={formData.id_place}\r\n                    onChange={handleChange}\r\n                    required\r\n                />\r\n\r\n                <label htmlFor=\"id_category\">Category ID</label>\r\n                <input\r\n                    type=\"number\"\r\n                    id=\"id_category\"\r\n                    name=\"id_category\"\r\n                    value={formData.id_category}\r\n                    onChange={handleChange}\r\n                    required\r\n                />\r\n\r\n                <label htmlFor=\"max_amount\">Max Participants</label>\r\n                <input\r\n                    type=\"number\"\r\n                    id=\"max_amount\"\r\n                    name=\"max_amount\"\r\n                    value={formData.max_amount}\r\n                    onChange={handleChange}\r\n                    required\r\n                />\r\n\r\n                <label htmlFor=\"image\">Image URL</label>\r\n                <input\r\n                    type=\"url\"\r\n                    id=\"image\"\r\n                    name=\"image\"\r\n                    value={formData.image}\r\n                    onChange={handleChange}\r\n                    placeholder=\"Enter image URL or upload a file below\"\r\n                />\r\n\r\n                <label htmlFor=\"upload\">Upload Image</label>\r\n                <input type=\"file\" id=\"upload\" accept=\"image/*\" onChange={handleImageUpload} />\r\n\r\n                <button type=\"submit\" className=\"submit-btn\" disabled={loading}>\r\n                    {loading ? 'Submitting...' : 'Add Event'}\r\n                </button>\r\n            </form>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default AddPage;\r\n"],"mappings":";;AAAA,OAAOA,KAAK,IAAIC,QAAQ,QAAQ,OAAO;AACvC,SAASC,WAAW,QAAQ,kBAAkB;AAC9C,OAAO,eAAe,CAAC,CAAC;AACxB,OAAOC,eAAe,MAAM,0BAA0B,CAAC,CAAC;AAAA,SAAAC,MAAA,IAAAC,OAAA;AAExD,MAAMC,OAAO,GAAGA,CAAA,KAAM;EAAAC,EAAA;EAClB,MAAMC,QAAQ,GAAGN,WAAW,CAAC,CAAC;EAC9B,MAAM,CAACO,QAAQ,EAAEC,WAAW,CAAC,GAAGT,QAAQ,CAAC;IACrCU,IAAI,EAAE,EAAE;IACRC,WAAW,EAAE,EAAE;IACfC,IAAI,EAAE,EAAE;IACRC,QAAQ,EAAE,EAAE;IACZC,WAAW,EAAE,EAAE;IACfC,UAAU,EAAE,EAAE;IACdC,KAAK,EAAE;EACX,CAAC,CAAC;EACF,MAAM,CAACC,KAAK,EAAEC,QAAQ,CAAC,GAAGlB,QAAQ,CAAC,EAAE,CAAC;EACtC,MAAM,CAACmB,OAAO,EAAEC,UAAU,CAAC,GAAGpB,QAAQ,CAAC,KAAK,CAAC;;EAE7C;EACA,MAAMqB,YAAY,GAAIC,CAAC,IAAK;IACxB,MAAM;MAAEZ,IAAI;MAAEa;IAAM,CAAC,GAAGD,CAAC,CAACE,MAAM;IAChCf,WAAW,CAAC;MAAE,GAAGD,QAAQ;MAAE,CAACE,IAAI,GAAGa;IAAM,CAAC,CAAC;EAC/C,CAAC;;EAED;EACA,MAAME,iBAAiB,GAAIH,CAAC,IAAK;IAC7B,MAAMI,IAAI,GAAGJ,CAAC,CAACE,MAAM,CAACG,KAAK,CAAC,CAAC,CAAC;IAC9B,IAAI